<ul class="<%= container_class %>">
  <% if @vehicles.present? %>
    <% @vehicles.each do |vehicle| %>
      <%= render(
        @component_class.new(
          bike: vehicle,
          current_user: @current_user,
          skip_cache: @skip_cache,
          search_kind: @search_kind,
        ),
      ) %>
    <% end %>
  <% else %>
    <%# Legacy views render themselves %>
    <%= content %>
  <% end %>
</ul>
