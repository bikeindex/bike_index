<div class="container mb-5">
  <div class="row">
    <div class="col-md-12">
      <% if @bikes.present? %>
        <ul class="bike-boxes">
          <%= render partial: '/bikes/bike', collection: @bikes, cached: true %>
        </ul>
      <% else %>
        <h3 class="text-center uncap">
          <%#= t(".no_bikes_matched") %>
        </h3>
      <% end %>
    </div>
  </div>
</div>
